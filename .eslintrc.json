{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "script"
  },
  "globals": {
    "MIDI": "readonly",
    "Abc": "readonly",
    "Midi": "readonly",
    "Share": "readonly",
    "module": "readonly",
    "require": "readonly"
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": ["warn", { "args": "none", "varsIgnorePattern": "^_" }],
    "no-undef": "error"
  }
}

